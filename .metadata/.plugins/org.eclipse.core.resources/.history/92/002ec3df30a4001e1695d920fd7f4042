package rs.ac.bg.etf.pp1;

public class sym {
	// Keywords
	public static final int PROG = 1;
	public static final int NEW = 2;
	public static final int PRINT = 3;
	public static final int READ = 4;
	public static final int NAMESPACE = 5;


	
	// Identifiers
	public static final int IDENT = 6;
	
	// Constants
	public static final int NUMBER = 7;
	public static final int CHARCONST = 8;
	public static final int BOOLCONST = 9;

	
	// Operators
	public static final int PLUS = 10;
	public static final int EQUAL = 11;
	public static final int COMMA = 12;
	public static final int SEMI = 13;
	public static final int LPAREN = 14;
	public static final int RPAREN = 15;
	public static final int LBRACE = 16;
	public static final int RBRACE = 17;
	public static final int LBRACKET = 18;
	public static final int RBRACKET = 19;
	public static final int MINUS = 20;
	public static final int MUL = 21;
	public static final int DIV = 22;
	public static final int MOD = 23;
	public static final int INC = 24;
	public static final int DEC = 25;
	public static final int DOUBLECOLON = 26;
	public static final int CONST = 27;


	
	public static final int EOF = 28;
	public static final int VOID = 29;
}
